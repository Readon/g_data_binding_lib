namespace GDataGtk
{
	/**
	 * Specifies string display character case conversion
	 * 
	 * @since 0.1
	 */
	public enum CharacterCaseMode
	{
		/**
		 * Leave unchanged
		 * 
		 * @since 0.1
		 */
		UNCHANGED,
		/**
		 * Convert to upcase string
		 * 
		 * @since 0.1
		 */
		UPCASE,
		/**
		 * Convert to locase string
		 * 
		 * @since 0.1
		 */
		LOCASE
	}
}
