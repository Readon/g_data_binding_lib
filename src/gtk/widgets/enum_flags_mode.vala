namespace GDataGtk
{
	/**
	 * Specifies display mode for enum/flags value types in string
	 * 
	 * @since 0.1
	 */
	public enum EnumFlagsMode
	{
		/**
		 * Represented by numeric value
		 * 
		 * @since 0.1
		 */
		NUMBER,
		/**
		 * Represented by enum/flag value nick
		 * 
		 * @since 0.1
		 */
		NICK,
		/**
		 * Represented by enum/flag value name
		 * 
		 * @since 0.1
		 */
		NAME,
		/**
		 * Represented by custom method resolving
		 * 
		 * @since 0.1
		 */
		CUSTOM
	}
}
